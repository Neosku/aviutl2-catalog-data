      "source": {
        "direct": "https://spring-fragrance.mints.ne.jp/aviutl/AviUtl2beta11a_setup.exe"
      },
      "install": [
        {
          "action": "download"
        },
        {
          "action": "run",
          "path": "{download}"
        }
      ],
      "uninstall": [
        {
          "action": "run",
          "path": "{appDir}/unins000.exe",
          "args": [
