    "id": "Kenkun.AviUtlExEdit2",
    "latest-version": "beta15",
    "checkType": {
      "type": "URL",
      "url": "https://spring-fragrance.mints.ne.jp/aviutl/",
      "regex": "aviutl2(beta[^.]+)\\.zip$",
      "version_extract": "beta[^.]+",
      "directTemplate": "https://spring-fragrance.mints.ne.jp/aviutl/AviUtl2[version_extract]_setup.exe",
      "hash-calc": [
        {
          "extract": "zip",
          "paths": [
            "aviutl2.exe"
          ],
          "recordPaths": [
            "{appDir}/aviutl2.exe"
          ]
        }
      ]
    }
  },
